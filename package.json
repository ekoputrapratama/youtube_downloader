{
  "name": "yt-dlp-gui",
  "author": "Muhammad Sayuti <muhammad.sayuti94@gmail.com>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ekoputrapratama/yt-dlp-gui"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init && npx playwright install && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "vite": "vite",
    "dev": "concurrently \"vite\"  \"python -m youtube_downloader --debug --url=http://localhost:5173\"",
    "develop": "vite | python -m youtube_downloader --debug --url=http://localhost:5173",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.7",
    "@mui/x-data-grid": "^8.27.0",
    "dayjs": "^1.11.19",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@headlessui/react": "^2.2.9",
    "@hookform/devtools": "^4.4.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "4.0.15",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.4",
    "prop-types": "^15.8.1",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.3.1",
    "vite-plugin-on-success": "^1.0.1",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
